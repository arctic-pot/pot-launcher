{
  "name": "pot-launcher",
  "productName": "pot-launcher",
  "version": "1.0.0",
  "description": "A modern Minecraft launcher with localization",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "format:write": "prettier --write \"src/**/*.{tsx,ts,js}\"",
    "format:check": "prettier --list-different \"src/**/*.{tsx,ts,js}\"",
    "format": "npm run format:check || npm run format:write",
    "code-style-check": "npm run format:check && npm run lint"
  },
  "keywords": [],
  "author": {
    "name": "TheColdPot",
    "email": "guohw979@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pot_launcher"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@electron-forge/plugin-webpack": "6.0.0-beta.57",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/crypto-js": "^4.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/graceful-fs": "^4.1.5",
    "@types/progress-stream": "^2.0.2",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "^1.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.0",
    "dart-sass": "^1.25.0",
    "electron": "^13.1.1",
    "eslint": "^7.6.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "node-sass": "^6.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "animate.css": "^4.1.1",
    "classnames": "^2.3.1",
    "compressing": "^1.5.1",
    "crypto-js": "^4.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "graceful-fs": "^4.2.6",
    "node-watch": "^0.7.1",
    "notistack": "^1.0.9",
    "progress-stream": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.2",
    "react-router-dom": "^5.2.0",
    "synp": "^1.9.7",
    "uuid": "^8.3.2"
  }
}
